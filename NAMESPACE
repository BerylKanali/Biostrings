useDynLib(Biostrings)

exportClasses(
    "BString", "DNAString", "RNAString",
    "BStringViews"
)

exportMethods(
    ## Generics defined elsewhere
    initialize, length, "[", "[[", show, "==", "!=", as.integer, toString,
    ## New generics
    matchPattern, countPattern, mismatch
)

export(
    debug_utils, debug_bbuf, debug_shiftor,
    #bbuf, bbCopy, ibuf, ibReadInts, ibWriteInts,
    BString, subBString, DNAString, RNAString,
    BStringViews, subject, first, last, width, desc,
    readFASTA, DNAStringViews, RNAStringViews,
    matchDNAPattern
)
