    "BString", "DNAString", "RNAString",
    "BStringViews"
)

exportMethods(
    ## Generics defined elsewhere
    initialize, length, as.integer, toString, "[", "[[", show, "==", "!=",
    ## New generics
    BStringViews, matchPattern, countPattern, mismatch
)

export(
    readFASTA,
    debug_utils, debug_bbuf, debug_shiftor,
    #bbuf, bbCopy, ibuf, ibReadInts, ibWriteInts,
    BString, subBString, DNAString, RNAString,
    subject, first, last, width, desc,
    views, adjacentViews,
    matchDNAPattern
)
